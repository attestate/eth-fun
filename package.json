{
  "name": "eth-fun",
  "version": "0.4.0",
  "description": "A treeshake-ready, functional library of Ethereum utility functions",
  "main": "dist/eth-fun.js",
  "type": "module",
  "scripts": {
    "build:node": "esbuild src/index.js --outfile=dist/eth-fun.js --sourcemap --bundle --platform=node --target=node14.16.1",
    "git:add": "git update-index --add dist/eth-fun.js dist/eth-fun.js.map",
    "test": "ava --verbose --timeout 20s"
  },
  "pre-commit": [
    "test",
    "build:node",
    "git:add"
  ],
  "ava": {
    "nodeArguments": [
      "--loader=quibble",
      "--no-warnings"
    ]
  },
  "keywords": [
    "ethereum",
    "solidity",
    "utility",
    "functional"
  ],
  "author": "Tim Daubensch√ºtz <tim@daubenschuetz.de> (https://timdaub.github.io/)",
  "license": "GPL-3.0-only",
  "dependencies": {
    "cross-fetch": "3.1.4",
    "solc": "0.6.12",
    "test-readme-md": "0.0.3",
    "web3-eth-abi": "1.4.0"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "esbuild": "^0.12.28",
    "fetch-mock": "9.11.0",
    "pre-commit": "1.2.2",
    "quibble": "0.6.5"
  }
}
