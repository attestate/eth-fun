{
  "name": "eth-fun",
  "version": "0.10.0",
  "description": "A treeshake-ready, functional library of Ethereum utility functions",
  "type": "module",
  "main": "./cjs/index.js",
  "engines": {
    "node": ">=20.0.0 <22.0.0",
    "npm": ">=10.0.0"
  },
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "default": "./src/index.js"
    }
  },
  "module": "./src/index.js",
  "scripts": {
    "build:node": "node build.js",
    "git:add": "git update-index --add cjs/*",
    "test": "ava --verbose --timeout 20s"
  },
  "pre-commit": [
    "build:node",
    "test",
    "git:add"
  ],
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@ava/babel/preset-instance"
        ]
      }
    },
    "nodeArguments": [
      "--loader=esmock",
      "--no-warnings"
    ]
  },
  "keywords": [
    "ethereum",
    "solidity",
    "utility",
    "functional"
  ],
  "author": "Tim Daubensch√ºtz <tim@daubenschuetz.de> (https://timdaub.github.io/)",
  "license": "GPL-3.0-only",
  "dependencies": {
    "cross-fetch": "3.1.4",
    "web3-eth-abi": "1.4.0"
  },
  "devDependencies": {
    "@ava/babel": "2.0.0",
    "abort-controller": "3.0.0",
    "ava": "6.4.1",
    "esbuild": "^0.12.28",
    "esmock": "^2.6.3",
    "fetch-mock": "^9.11.0",
    "pre-commit": "1.2.2",
    "test-readme-md": "0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rugpullindex/eth-fun.git"
  },
  "bugs": {
    "url": "https://github.com/rugpullindex/eth-fun/issues"
  },
  "homepage": "https://github.com/rugpullindex/eth-fun#readme"
}
